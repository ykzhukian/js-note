{
  "extends": ["tslint-react", "tslint-config-airbnb"],
  "rules": {
    "interface-name": false,
    "variable-name": false,
    "jsx-no-multiline-js": false,
    "import-name": false,
    "no-increment-decrement": false,
    "ter-arrow-parens": false,
    "semicolon": false,
    "jsx-no-lambda": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "no-implicit-dependencies": [false, ["src"]]
  }
}
